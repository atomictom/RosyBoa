def mod(a, b) {
  n = a / b
  return (a - (n * b))
}

def fizzbuzz(n) {
  i = 1
  while i <= n {
    if mod(i, 15) == 0 {
      println "Fizzbuzz"
    } elif mod(i, 5) == 0 {
      println "Buzz"
    } elif mod(i, 3) == 0 {
      println "Fizz"
    } else {
      println i
    }
    i = i + 1
  }
}

fizzbuzz(30)

def mod(a, b) {
  n = a / b
  return (a - (n * b))
}

def fizzbuzz(n) {
  i = 1
  while i <= n {
    if mod(i, 15) == 0 {
      print "Fizzbuzz\n"
    } elif mod(i, 5) == 0 {
      print "Buzz\n"
    } elif mod(i, 3) == 0 {
      print "Fizz\n"
    } else {
      print i
      print "\n"
    }
    i = i + 1
  }
}

fizzbuzz(30)
